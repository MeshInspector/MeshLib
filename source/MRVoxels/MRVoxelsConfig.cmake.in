@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

set(MESHLIB_USE_VCPKG @MESHLIB_USE_VCPKG@)
if(CMAKE_SYSTEM_NAME STREQUAL "Linux" AND NOT MESHLIB_USE_VCPKG)
  # Ubuntu packages have non-standard location for OpenVDB
  set(OPENVDB_INCLUDEDIR "@PACKAGE_MR_INCLUDE_DIR@/include")
  set(OPENVDB_LIBRARYDIR "@PACKAGE_MR_MAIN_LIB_DIR@/lib")
endif()
find_dependency(OpenVDB 10)

# static builds require to find private dependencies
if(EMSCRIPTEN)
  set(MRVOXELS_NO_DICOM @MRVOXELS_NO_DICOM@)
  if(NOT MRVOXELS_NO_DICOM)
    # GDCM config redefines SELF_DIR which breaks subsequent includes
    set(SELF_DIR_BACKUP ${SELF_DIR})
    find_dependency(GDCM)
    set(SELF_DIR ${SELF_DIR_BACKUP})
  endif()
endif()

include("${CMAKE_CURRENT_LIST_DIR}/MRVoxelsTargets.cmake")

check_required_components(MRVoxels)
