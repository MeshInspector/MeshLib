@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
find_dependency(Boost 1.73)
find_dependency(Eigen3)
find_dependency(fmt 8)
find_dependency(JsonCpp)
find_dependency(spdlog 1.9)
find_dependency(TBB 2020.1)
find_dependency(tl-expected)

# static builds require to find private dependencies
if(EMSCRIPTEN)
  # libzip on Emscripten is unable to resolve its dependencies by itself
  find_package(MbedTLS 1.0 REQUIRED)
  find_package(ZLIB REQUIRED)
  find_dependency(libzip)

  set(MRMESH_NO_GTEST @MRMESH_NO_GTEST@)
  if(NOT MRMESH_NO_GTEST)
    find_dependency(GTest)
  endif()
endif()

include("${CMAKE_CURRENT_LIST_DIR}/MRMeshTargets.cmake")

check_required_components(MRMesh)
